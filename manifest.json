{
    "name": "List tabs",
    "description": "Lists all tabs",
    "manifest_version": 2,
    "version": "0.2",

    "icons": {
        "32": "icon.png"
    },

    "permissions": ["tabs", "activeTab"],

    "background": {
        "scripts": ["list_tabs.js"]
    },

    "browser_action": {
        "default_icon": {
            "32": "icon.png"
        }
    },

    "web_accessible_resources": ["page.html"]
}
